        }
        .tagsBox {
            display: inline-block;
            position: relative;
            height:30px;
            white-space: nowrap;
            margin-right: 5px;
            margin-bottom: 10px;
            vertical-align: top;

        }
        .deletTagsBox {
            position: absolute;
            display: inline-block;
            right: 5px;
            top: 5px;
            color: #c3c3c3;
            line-height: 10px;
            font-size: 20px;
            font-weight: bold;
            display: none;
            cursor: pointer;
        }
        .addTags {
          display: inline-block;
            height: 30px;
            line-height: 30px;
            font-size: 24px;
            font-weight: bold;
            cursor: pointer;
            margin-bottom: 10px;
        }
        .tagsContent {
            display: inline-block;
            min-width: 80px;
            display: none;
            border: 1px solid #00a0ea;
            border-radius: 5px;
            height: 30px;
            box-sizing: border-box;
            line-height: 30px;
            cursor: pointer;
            padding: 0 7px;
        }
