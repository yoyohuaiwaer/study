        <div class="tagsList">
            <label for="tags" style="width: 75px; vertical-align: top">标签:</label>
            <div class="bigBox">
                <!--<div class="tagsBox">
                    <span class="deletTagsBox">x</span>
                    <span class="tagsContent" data-id=""></span>
                    <input type="text" class="text tags" data-id="" value=""/>
                </div><div class="tagsBox">
                    <span class="deletTagsBox">x</span>
                    <span class="tagsContent" data-id=""></span>
                    <input type="text" class="text tags" data-id=""  value=""/>
                </div><div class="tagsBox">
                    <span class="deletTagsBox">x</span>
                    <span class="tagsContent" data-id=""></span>
                    <input type="text" class="text tags" data-id="" value=""/>
                </div><div class="tagsBox">
                    <span class="deletTagsBox">x</span>
                    <span class="tagsContent" data-id=""></span>
                    <input type="text" class="text tags" data-id="" value=""/>
