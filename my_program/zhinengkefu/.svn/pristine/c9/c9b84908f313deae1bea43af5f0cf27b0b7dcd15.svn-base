@font-face {
    font-family: 'icomoon-boot';
    src:  url('fonts/icomoon-boot.eot?n8l070');
    src:  url('fonts/icomoon-boot.eot?n8l070#iefix') format('embedded-opentype'),
    url('fonts/icomoon-boot.ttf?n8l070') format('truetype'),
    url('fonts/icomoon-boot.woff?n8l070') format('woff'),
    url('fonts/icomoon-boot.svg?n8l070#icomoon-boot') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icomoon-boot' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

