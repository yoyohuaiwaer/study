input[type=range] {
    -webkit-appearance: none;
    /* margin: 18px 0;*/
    /*position: absolute;*/
    width: 100px;
    top: 14px;
}
input[type=range]:focus {
    outline: none;
}
input[type=range]::-webkit-slider-runnable-track {
    width: 100%;
    height: 3px;
    cursor: pointer;
    animate: 0.2s;
    background: #e3e3e3;
}
input[type=range]::-webkit-slider-thumb {
    height: 3px;
    width: 5px;
    border-radius: 3px;
    background: transparent;
    cursor: pointer;
    -webkit-appearance: none;
}
input[type=range]:focus::-webkit-slider-runnable-track {
    background: #367ebd;
}
.rangeSelect{
    position: absolute;
    content: '';
    z-index: 333;
    height: 3px;
    background: #009fe9;
    left: 81px;
    top: 14px;

}
/*-moz-document url-prefix() {.rangeSelect{ top:24px;}}*/
input[type=range]::-moz-range-track {
    width: 100%;
    height: 3px;
    cursor: pointer;
    animate: 0.2s;
    background: #e3e3e3;
}
input[type=range]::-moz-range-thumb {
    height: 0px;
    width: 0px;
    /*border-radius: 3px;*/
    background: transparent;
    cursor: pointer;
}
input[type=range]::-ms-track {
    width: 100%;
    height: 3px;
    cursor: pointer;
    animate: 0.2s;
    background: transparent;
    border-color: transparent;
    border-width: 16px 0;
    color: transparent;
}
input[type=range]::-ms-fill-lower {
    background: #50c2f6;
}
input[type=range]::-ms-fill-upper {
    background: #e3e3e3;
} input[type=range]::-ms-thumb {
      height: 3px;
      width: 5px;
      border-radius: 3px;
      background: transparent;
      cursor: pointer;
  }
input[type=range]:focus::-ms-fill-lower {
    background: #e3e3e3;
}
input[type=range]:focus::-ms-fill-upper {
    background: #367ebd;
}
