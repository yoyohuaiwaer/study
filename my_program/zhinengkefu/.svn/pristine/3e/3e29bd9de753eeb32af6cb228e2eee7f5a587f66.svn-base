.login-box {
  width: 475px;
  height: 520px;
  /*margin: 0 auto;*/
  box-shadow: 1px 0 5px rgba(0, 0, 0, 0.1), -1px 0 5px rgba(0, 0, 0, 0.1);
  background: #fff;
  font-size: 14px;
  position: absolute;
  border-radius: 4px;
  z-index: 0;
  /* display: inline-block; */
  /* text-align: center; */
  top: 50%;
  left: 50%;
  margin-top: -260px;
  margin-left: -237px;
}
.login-box:before,
.login-box:after {
  position: absolute;
  content: '';
  width: 48%;
  height: 10px;
  bottom: 6px;
  z-index: -1;
  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.5);
}
.login-box:before {
