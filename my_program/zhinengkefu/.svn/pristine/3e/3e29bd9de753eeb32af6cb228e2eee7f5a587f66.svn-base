* {
  margin: 0;
  padding: 0;
}
* html body {
  overflow: visible;
}
* html iframe,
* html frame {
  overflow: auto;
}
* html frameset {
  overflow: hidden;
}
body,
ul,
ol,
li,
p,
h1,
h2,
h3,
h4,
h5,
h6,
form,
fieldset,
table,
td,
img,
div,
tr {
  margin: 0;
  padding: 0;
}
table {
  border: 1px solid #efeff1;
}
th {
  font-weight: normal;
}
tr {
  text-align: center;
}
th,
td {
  text-align: center;
  border: 1px solid #efeff1;
  box-sizing: border-box;
  line-height: 45px;
  padding: 0 10px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
input,
select {
  font-size: 12px;
  vertical-align: middle;
}
select {
  height: 30px;
  line-height: 30px;
  border: 1px solid #c3c3c3;
  border-radius: 5px;
  min-width: 80px;
}
textarea,
input {
  word-wrap: break-word;
  word-break: break-all;
  padding: 0px;
}
input[readonly] {
  border: none;
  width: auto;
  min-width: 10px;
}
li {
  list-style-type: none;
}
img {
  border: 0 none;
}
a {
  color: #333333;
  text-decoration: none;
  outline: none;
  cursor: pointer;
}
a:hover {
  color: #ff6633;
}
h1 {
  font-size: 24px;
}
h2 {
  font-size: 20px;
}
h3 {
  font-size: 18px;
}
h4 {
  font-size: 16px;
}
h5 {
  font-size: 14px;
}
h6 {
  font-size: 12px;
}
.pointer {
  cursor: pointer;
}
.noborder {
  border: 0 none;
}
.noPadding {
  padding: 0;
}
.t_l {
  text-align: left;
}
.t_c {
  text-align: center;
}
.t_r {
  text-align: right;
}
.break {
  word-break: break-all;
  word-wrap: break-word;
}
.nobreak {
  word-break: keep-all;
  word-wrap: normal;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.unline {
  text-decoration: underline;
}
.disunline {
  text-decoration: none;
}
.absolute {
  position: absolute;
}
.relative {
  position: relative;
}
.wrapper {
  clear: both;
  overflow: hidden;
}
.o-hidden {
  overflow: hidden;
}
.invisible {
  visibility: hidden;
}
.hidden {
  display: none;
}
.block {
  display: block;
}
.clear {
  clear: both;
  height: 0px;
  width: 100%;
  font-size: 1px;
  line-height: 0px;
  visibility: hidden;
  overflow: hidden;
}
.fb {
  font-weight: bold;
}
.fn {
  font-weight: normal;
}
.fI {
  font-style: italic;
}
.f10 {
  font-size: 10px;
}
.f11 {
  font-size: 11px;
}
.f12 {
  font-size: 12px;
}
.f13 {
  font-size: 13px;
}
.f14 {
  font-size: 14px;
}
.f16 {
  font-size: 16px;
}
.f18 {
  font-size: 18px;
}
body,
#layout,
html {
